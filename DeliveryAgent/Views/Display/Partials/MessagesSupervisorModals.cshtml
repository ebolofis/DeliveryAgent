                        <div class="custom-flex-60 custom-flex-center messages-supervisor-edit-message-selection-container">
                            <button class="custom-default-button custom-default-button-simple custom-flex-row custom-flex-center custom-text-black custon-text-medium messages-supervisor-edit-message-selection-button" tabindex="-1" data-toggle="dropdown">
                                <div class="custom-flex-90 custom-flex-start" data-bind="text: $parent.SelectedMessage() != null ? $parent.SelectedMessage().Description : ''"></div>
                                <div class="custom-flex-10 custom-flex-center"><i class="fa fa-caret-down custom-fa-faint"></i></div>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-list dropdown-menu-list-left dropdown-menu-list-length-long" id="scrollbar-style" data-bind="foreach: Messages">
                                <li class="dropdown-menu-list-item" data-bind="event: { mousedown: $root.SelectMessage.bind($data, $data) }"><span data-bind="text: Description"></span></li>
                            </ul>
                        </div>
                    </div>
                    <div class="custom-flex-33 custom-flex-column" data-bind="with: SelectedMessage">
                        <div class="custom-flex-40 custom-flex-center custom-text-black" data-bind="text: language.Translate('RequestSubcategory', null)"></div>
                        <div class="custom-flex-60 custom-flex-center messages-supervisor-edit-message-selection-container">
                            <button class="custom-default-button custom-default-button-simple custom-flex-row custom-flex-center custom-text-black custon-text-medium messages-supervisor-edit-message-selection-button" tabindex="-1" data-toggle="dropdown">
                                <div class="custom-flex-90 custom-flex-start" data-bind="text: $parent.SelectedMessageDetail() != null ? $parent.SelectedMessageDetail().Description : ''"></div>
                                <div class="custom-flex-10 custom-flex-center"><i class="fa fa-caret-down custom-fa-faint"></i></div>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-list dropdown-menu-list-left dropdown-menu-list-length-long" id="scrollbar-style" data-bind="foreach: MessageDetails">
                                <li class="dropdown-menu-list-item" data-bind="event: { mousedown: $root.SelectMessageDetail.bind($data, $data) }"><span data-bind="text: Description"></span></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="custom-flex-30 custom-flex-row">
                    <div class="custom-flex-30 custom-flex-start custom-text-black" data-bind="text: language.Translate('RequestMessageColon', null)"></div>
                    <div class="custom-flex-70 custom-flex-center">
                        <textarea class="custom-input custom-text-black messages-supervisor-edit-message-message-description-input" id="scrollbar-style" data-bind="value: MessageDescription, attr: { 'placeholder' : language.Translate('RequestMessage', null)}" type="text" autocomplete="off" tabindex="-1"></textarea>
                    </div>
                </div>
                <div class="custom-flex-20 custom-flex-row">
                    <div class="custom-flex-40 custom-flex-column">
                        <div class="custom-flex-40 custom-flex-center custom-text-black" data-bind="text: language.Translate('Order', null)"></div>
                        <div class="custom-flex-60 custom-flex-center messages-supervisor-edit-message-selection-container">
