        <div class="custom-flex-74">
            <div class="custom-flex-column custom-full-width messages-supervisor-edit-message-container">
                <div class="custom-flex-20 custom-flex-row">
                    <div class="custom-flex-34 custom-flex-column">
                        <div class="custom-flex-40 custom-flex-center custom-text-black" data-bind="text: language.Translate('RequestReason', null)"></div>
                        <div class="custom-flex-60 custom-flex-center messages-supervisor-edit-message-selection-container">
                            <button class="custom-default-button custom-default-button-simple custom-flex-row custom-flex-center custom-text-black custon-text-medium messages-supervisor-edit-message-selection-button" tabindex="-1" data-toggle="dropdown">
                                <div class="custom-flex-90 custom-flex-start" data-bind="text: SelectedMainMessage() != null ? SelectedMainMessage().Description : ''"></div>
                                <div class="custom-flex-10 custom-flex-center"><i class="fa fa-caret-down custom-fa-faint"></i></div>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-list dropdown-menu-list-left dropdown-menu-list-length-long" id="scrollbar-style" data-bind="foreach: MainMessages">
