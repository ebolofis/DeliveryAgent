                            </ul>
                        </div>
                    </div>
                    <div class="custom-flex-10 custom-flex-center">
                        <button class="custom-default-button custom-default-button-error custom-flex-center custom-text-white messages-supervisor-edit-message-cancel-selection-button" data-bind="click: $root.ClearCustomerMessageStore, clickBubble: false" tabindex="-1"><i class="fa fa-times custom-fa-faint"></i></button>
                    </div>
                </div>
                <div class="custom-flex-30 custom-flex-row">
                    <div class="custom-flex-30 custom-flex-start custom-text-black" data-bind="text: language.Translate('RequestCustomerNoteColon', null)"></div>
                    <div class="custom-flex-70 custom-flex-center">
                        <textarea class="custom-input custom-text-black messages-supervisor-edit-message-customer-description-input" id="scrollbar-style" data-bind="value: CustomerDescription, attr: { 'placeholder' : language.Translate('RequestCustomerNote', null)}" type="text" autocomplete="off" tabindex="-1"></textarea>
