                            <button class="custom-default-button custom-default-button-simple custom-flex-row custom-flex-center custom-text-black custon-text-medium messages-supervisor-edit-message-selection-button" tabindex="-1" data-toggle="dropdown">
                                <div class="custom-flex-90 custom-flex-start"><span data-bind="text: SelectedStore() != null ? $root.storeOptions()[SelectedStore().Id] : ''"></span></div>
                                <div class="custom-flex-10 custom-flex-center"><i class="fa fa-caret-down custom-fa-faint"></i></div>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-list dropdown-menu-list-left dropdown-menu-list-length-small" id="scrollbar-style" data-bind="foreach: $root.stores">
