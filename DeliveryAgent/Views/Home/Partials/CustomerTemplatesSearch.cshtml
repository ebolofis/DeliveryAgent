            </div>
        </div>
        <div class="custom-flex-10 custom-flex-row">
            <div class="custom-flex-70"></div>
            <div class="custom-flex-10 custom-flex-center">
                <button class="custom-default-button custom-default-button-simple custom-text-black customer-orders-multitude" data-bind="click: $root.SelectOrdersMultitude.bind($data, 5), clickBubble: false, css: { 'custom-default-button-info' : $root.customerOrdersMultitude() == 5 }" tabindex="-1"><div>5</div></button>
            </div>
            <div class="custom-flex-10 custom-flex-center">
                <button class="custom-default-button custom-default-button-simple custom-text-black customer-orders-multitude" data-bind="click: $root.SelectOrdersMultitude.bind($data, 10), clickBubble: false, css: { 'custom-default-button-info' : $root.customerOrdersMultitude() == 10 }" tabindex="-1"><div>10</div></button>
            </div>
            <div class="custom-flex-10 custom-flex-center">
                <button class="custom-default-button custom-default-button-simple custom-text-black customer-orders-multitude" data-bind="click: $root.SelectOrdersMultitude.bind($data, 20), clickBubble: false, css: { 'custom-default-button-info' : $root.customerOrdersMultitude() == 20 }" tabindex="-1"><div>20</div></button>
            </div>
        </div>
    </div>
</script>

<script type="text/html" id="customer-tab-messages-template">
    <div class="custom-full-height custom-flex-column" data-bind="with: $root.customerMessagesPagination">
        <div class="custom-flex-10">
            <div class="custom-full-width custom-flex-row customer-messages-header">
                <div class="custom-flex-12 custom-flex-start custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('Store', null)"></div>
                <div class="custom-flex-11 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('Staff', null)"></div>
                <div class="custom-flex-10 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('Creation', null)"></div>
                <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('RequestReason', null)"></div>
                <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('RequestCategory', null)"></div>
                <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('RequestSubcategory', null)"></div>
                <div class="custom-flex-18 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('RequestMessage', null)"></div>
                <div class="custom-flex-7 custom-flex-center custom-text-black custom-text-bold custom-text-small" data-bind="text: language.Translate('Actions', null)"></div>
            </div>
        </div>
        <!-- ko if: entityList().length > 0 -->
        <div class="custom-flex-75 custom-flex-column" id="scrollbar-style" data-bind="foreach: entityList">
            <div>
                <div class="custom-flex-row pagination-entity-list-entity">
                    <div class="custom-flex-12 custom-flex-start custom-text-black custom-text-extra-small" data-bind="text: (StoreId != null ? $root.storeOptions()[StoreId] : '')"></div>
                    <div class="custom-flex-11 custom-flex-center custom-text-black custom-text-extra-small" data-bind="text: (StaffFirstName != null ? StaffFirstName : '') + (StaffFirstName != null && StaffLastName != null ? ' ' : '') + (StaffLastName != null ? StaffLastName : '')"></div>
                    <div class="custom-flex-10 custom-flex-center custom-text-black custom-text-extra-small" data-bind="text: moment.utc(CreationDate).format('DD-MM-YYYY HH:mm')"></div>
                    <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-small" data-bind="text: (MainMessage != null ? MainMessage : '')"></div>
                    <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-small" data-bind="text: (Message != null ? Message : '')"></div>
                    <div class="custom-flex-14 custom-flex-center custom-text-black custom-text-small" data-bind="text: (MessageDetail != null ? MessageDetail : '')"></div>
                    <div class="custom-flex-18 custom-flex-start custom-text-blue custom-text-small" data-bind="text: (Description != null ? Description : '')"></div>
                    <div class="custom-flex-7 custom-flex-center">
                        <button class="custom-default-button custom-default-button-success customer-messages-button" data-bind="click: $root.EditCustomerMessage.bind($data, $data), clickBubble: false, visible: IsTemporary" tabindex="-1"><i class="fa fa-edit custom-fa-faint"></i></button>
                    </div>
                </div>
            </div>
        </div>
        <div class="custom-flex-10 custom-flex-center">
            <!-- ko if: totalPages() > 1 && !scrollable() -->
            <ul class="pagination pagination-pagination">
                <li><a href="#" data-bind="click: MoveFirst, clickBubble: false">&laquo;</a></li>
                <li><a href="#" data-bind="click: MovePrevious, clickBubble: false">&lt;</a></li>
                <!-- ko if: hasLeft() -->
                <li>
                    <a href="#"><span>...</span></a>
                </li>
                <!-- /ko -->
                <!-- ko foreach: new Array(totalPages()) -->
                <!-- ko if: ($index() >= $parent.activePage() && $index() <= $parent.activePage() + 2) || ($index() <= $parent.activePage() && $index() >= $parent.activePage() - 2) -->
                <li data-bind="css: { active: ($index()) == $parent.activePage() }"><a href="#" data-bind="text: $index() + 1, click: $parent.MoveSpecific.bind($data, $index()), clickBubble: false"></a></li>
                <!-- /ko -->
                <!-- /ko -->
                <!-- ko if: hasRight() -->
                <li>
                    <a href="#"><span>...</span></a>
                </li>
                <!-- /ko -->
                <li><a href="#" data-bind="click: MoveNext, clickBubble: false">&gt;</a></li>
                <li><a href="#" data-bind="click: MoveLast, clickBubble: false">&raquo;</a></li>
            </ul>
            <!-- /ko -->
        </div>
        <div class="custom-flex-5 custom-flex-end custom-text-black">
            <div>*</div>&nbsp;<div class="custom-text-bold custom-text-large" data-bind="text: totalResults()"></div>&nbsp;<div data-bind="text: language.Translate('Found', null)"></div>
        </div>
        <!-- /ko -->
        <!-- ko if: entityList().length == 0 -->
        <div class="custom-flex-90">
        </div>
        <!-- /ko -->
    </div>
</script>
